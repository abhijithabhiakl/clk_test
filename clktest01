#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 10;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x55874eb00e20 .scope module, "clkGen" "clkGen" 2 2;
 .timescale -8 -10;
    .port_info 0 /OUTPUT 1 "clk";
P_0x55874eb01ea0 .param/l "period" 0 2 3, +C4<00000000000000000000000000110010>;
v0x55874eb027b0_0 .var "clk", 0 0;
    .scope S_0x55874eb00e20;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55874eb027b0_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x55874eb00e20;
T_1 ;
    %delay 2500, 0;
    %load/vec4 v0x55874eb027b0_0;
    %inv;
    %store/vec4 v0x55874eb027b0_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x55874eb00e20;
T_2 ;
    %delay 10000, 0;
    %vpi_call 2 13 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "clkTest.v";
